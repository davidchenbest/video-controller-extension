{
    "manifest_version": 2,
    "name": "video speed",
    "version": "1.0",
    "description": "video speed control",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "all_frames": true,
            "css": [
                "css/main.css"
            ],
            "js": [
                "js/main.js"
            ]
        }
    ]
}